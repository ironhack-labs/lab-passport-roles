<form action="/portal/admin/<%= id %>/editEmployee" method="post">
  <label for="username">Username</label><br>
  <input type="text" id="username" name="username" value="<%= username %>"><br>
  <label for="name">Name</label><br>
  <input type="text" id="name" name="name" value="<%= name %>"><br>
  <label for="familyName">FamilyName</label><br>
  <input type="familyName" id="familyName" name="familyName" value="<%= familyName %>"><br>
  <label for="password">Password</label><br>
  <input type="password" id="password" name="password" value="<%= password %>"><br>
  <select name="role">
   <option value="<%= role %>" selected><%= role %></option>
   <option value="Developer">Developer</option>
   <option value="TA">TA</option>
   <option value="Alumni">Alumni</option>
 </select>
  <br>
  <button> Modify user</button>
</form>
<a href="/portal/admin"><button>Go Back</button></a>
