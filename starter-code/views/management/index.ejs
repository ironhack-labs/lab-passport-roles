<h2>User Management</h2>

<ul>
  <% users.forEach((user) => { %>
    <li><b>Username:</b> <%= user.username %></li>
    <li><b>Name:</b> <%= user.name %></li>
    <li><b>Role:</b> <%= user.role %></li>
    <li><form action="/management/<%= user._id %>/delete" method="post"><button type="submit">Delete</button></form></li>
  <% }) %>
</ul>


<a href="/management">Go back to management page</a>
