<h2>Employees list</h2>
<ul>
  <% if (typeof docs !== "undefined") { %>
    <% docs.forEach(employee => { %>
      <% if (employee.role !== "Boss") { %>
        <li>Username: <%= employee.username %></li>
        <a href="/remove/<%= employee._id %>">Remove</a>
      <% } %>
    <% }); %>
  <% } %>
</ul>
<a href="/add">Add employee</a>
