<form action="update/<%= user._id %>" method="POST">
    <label for="name">Name</label>
    <input type="text" id="name" name="name" value="<%= user.name %>">
  
    <label for="familyName">familyName</label>
    <input type="text" name="familyName" id="familyName" value="<%= user.familyName %>">
  
    <label for="password">password URL</label>
    <input type="text" name="password" id="password" value="<%= user.password %>">
  
    <label for="role">role</label>
    <input type="text" name="role" id="role" value="<%= user.role %>">
  
    <button type="submit">Update Product</button>
  </form>