<h1> Ironhack Users </h1>
    <a href="/"> Back to menu </a> <br>
    <a href="/users/new"> Create a new User </a>
<ul>
    <% users.forEach((userData) => { %>
        <h3>  <%= userData.name %> </h3>
         <form action="/users/<%= userData._id%>/delete" method="POST">
                <button type="submit"> Delete an user </button>
            </form>
        <a href="/users/<%= userData._id %>/edit"> Edit an user </a>
        
        <br>

        <li> Family Name: <%= userData.familyname %> </li>
        <li> Role: <%= userData.role %> </li>
        <li> Ability: <%= userData.ability %> </li>
        
  <%  }); %>

    

</ul>