<h2>Manage the platform users:</h2>
<ul>
  <% users.forEach((user) => { %>
    <li>
      <%= user.name %> - <%= user.familyName %>
      <a href="/profile/<%= user._id %>">View Profile</a>
      <form action="/setupUsers/<%= user._id %>/delete" method="POST">
        <button type="submit">Delete User</button>
      </form>
    </li>
  <%  }); %>
</ul>

<a href="/newUser">Create a new user</a>
