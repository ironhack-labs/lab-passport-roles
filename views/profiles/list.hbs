<h1>Profiles<a href="/courses">Back</a></h1>
<table width="100%" cellspacing="0" cellpadding="10" border="0">
    <thead>
        <tr bgcolor="#ddd">
            <td>Students</td>
            <td>Actions</td>
        </tr>
    </thead>
    <tbody>
        {{#each users}}
          {{#ifEquals this.role  "STUDENT"}}
            <tr>
                <td>{{this.username}}</td>
                <td width="250"> 
                    <a class="view" href="/profile/{{this._id}}">View Profile</a>
                </td>
            </tr>
           {{/ifEquals}}
        {{/each}}
    </tbody>
</table>

